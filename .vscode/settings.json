{
  "eslint.enable": true,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "yaml",
    "vue",
    "markdown",
    "md",
    "json",
    "jsonc"
  ],
  "json.schemaDownload.enable": true,
  "npm.packageManager": "pnpm"
}
